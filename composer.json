{
    "require": {
        "grpc/grpc": "^v1.3.0",
        "php": ">=7.1",
        "spiral/php-grpc": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1",
        "phpstan/phpstan": "^0.10",
        "squizlabs/php_codesniffer": "3.*",
        "symplify/monorepo-builder": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Timitao\\GRPC\\Client\\": "packages/Client/.",
            "Timitao\\GRPC\\Server\\": "packages/Server/src/",
            "\\": "packages/PHPContract/src/"
        }
    },
    "replace": {
        "timitao/grpc-contract": "self.version",
        "timitao/grpc-server": "self.version",
        "timitao/grpc-client": "self.version",
        "timitao/grpc-php-contract": "self.version"
    }
}
